
.home-list{
	padding: 20rpx;
}
.home-list-hover{
	background-color: #F4F4F4;
}
.home-list-item{
	padding: 20rpx;
	border-top: 1rpx solid #EEEEEE;
	border-bottom: 1rpx solid #EEEEEE;
}
.home-list-item>view:first-child{
	color: #333333;
}
.home-list-item>view:first-child>view{
	margin-right: 10rpx;
}
.home-list-item>view:last-child{
	color: #CCCCCC;
}
.other-login{
	padding: 20rpx 80rpx;
}
.other-login>view>view{
	width: 100rpx;
	height: 100rpx;
	color: #FFFFFF;
	border-radius: 100%;
	font-size: 55rpx;
}
.other-login .icon-weixin{
	background-color: #2bd19b;
}
.other-login .icon-xinlangweibo{
	background-color: #fc7729;
}
.other-login .icon-QQ{
	background-color: #2caefc;
}
.home-data{
	padding: 20rpx 40rpx;
}
.home-data>view{
	color: #989898;
}
.home-data>view>view{
	color: #333333;
	font-size: 32rpx;
}
.home-adv{
	padding: 20rpx;
}
.home-adv>image{
	border-radius: 20rpx;
	width: 100%;
	height: 150rpx;
}
.home-info{
	padding: 20rpx 40rpx;
}
.home-info>image{
	-webkit-flex-shrink: 0;
	        flex-shrink: 0;
	width: 100rpx;
	height: 100rpx;
	border-radius: 100%;
	margin-right: 15rpx;
}
.home-info>view:first-of-type>view:first-child{
	font-size: 32rpx;
}
.home-info>view:first-of-type>view:last-child{
	color: #BBBBBB;
}
.home-info>view:last-of-type{
	/* width: 100%; */
	height: 100%;
	/* border: 1upx solid; */
}

