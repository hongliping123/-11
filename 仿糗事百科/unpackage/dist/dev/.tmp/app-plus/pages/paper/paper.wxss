
.body{
	padding: 0 20rpx;
}
.papoer-list{
	border-bottom: 1rpx solid #EEEEEE;
	padding: 0 20rpx 10rpx 0;
}
.papoer-list>image{
	width: 100rpx;
	height: 100rpx;
	border-radius: 100%;
	margin-right: 20rpx;
	-webkit-flex-shrink: 0;
	        flex-shrink: 0;
}
.papoer-list>view{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
}
.papoer-list>view>view:first-child{
	font-size: 35rpx;
}
.papoer-list>view>view:first-child>view{
	color: #CBCBCB;
}
.papoer-list>view>view:last-child{
	color: #999999;
}

